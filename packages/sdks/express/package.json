{
  "name": "@hypership/express",
  "version": "0.0.1",
  "description": "Hypership Express SDK",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "npm run build && pnpm link --global"
  },
  "author": "Hypership",
  "license": "ISC",
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.8",
    "@types/express": "^4.17.21",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "^5.5.2",
    "vite": "^6.0.3",
    "vite-plugin-dts": "^4.3.0"
  },
  "peerDependencies": {
    "express": "^4.x"
  },
  "homepage": "https://hypership.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/hypershiphq/hypership"
  },
  "bugs": {
    "url": "https://github.com/hypershiphq/hypership/issues"
  },
  "keywords": [
    "hypership",
    "express",
    "hypershiphq",
    "auth",
    "middleware"
  ]
}
