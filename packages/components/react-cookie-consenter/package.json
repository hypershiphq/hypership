{
  "name": "react-cookie-manager",
  "version": "1.0.0",
  "description": "Automatically block cookies and track consent status. GDPR compliant. Simple to use React component.",
  "keywords": [
    "react",
    "cookies",
    "cookie management",
    "react cookie banner",
    "react cookie consent",
    "react cookie manager",
    "gdpr",
    "cookie consent",
    "cookie blocking",
    "cookie consent manager",
    "cookie consent manager react",
    "analytics consent",
    "advertising consent",
    "social consent"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "tsup --config tsup.config.ts && npm link"
  },
  "type": "module",
  "author": "Hypership",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.17.16",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.5.2",
    "vite": "^6.0.3",
    "vite-plugin-dts": "^4.3.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
